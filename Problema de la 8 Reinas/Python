pos = [0] * 8;
i2 = 0;
posicionReinas(pos,i2)


def posicionReinas(pos, i2):
  if i2 < 8 :
    for j in range (1,8):
      i1 = 0
      while (i1 < i2 and pos[i1] != j and abs(i1 - i2) != abs(pos[i1] - j):
        i1 += 1;
      if i1 == i2:
        pos[i2] = j
        if posicionReinas(pos, i2 + 1)[i2] != 0:
          return pos
    if j == 9:
      por[i2 - 1] = 0
    return pos 
     
