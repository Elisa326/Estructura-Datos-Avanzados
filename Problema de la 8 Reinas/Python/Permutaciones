def permuta(p,disp,k,n, c):
    if k > n:
        c[0] = c[0] +1
        print(str(c[0])  + " â†’ " + str(p[1:]))
        
    else:
        for i in range (1, n+1):
            if disp[i] == 1:
                disp[i] = 0
                p[k] = i
                
                permuta(p,disp,k+1, n, c)
                disp[i] = 1
n = 8
p = [0] * (n+1)
d = [1] * (n+1)
