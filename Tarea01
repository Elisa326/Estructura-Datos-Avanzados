public class Tarea01{
	public static void main(String args[]){
		int n, num1, num2;
		
		n = Integer.parseInt(args[0]);
		for(int i = 0; i < n; i++){
			num1 = Integer.parseInt(args[2*i + 1]);
			num2 = Integer.parseInt(args[2*(i + 1)]);
			System.out.println(num1 + num2);
		}
	}
}



import java.io.File;
import java.util.Scanner;
public class Tarea01{

  public static String leeArchivo (String nom) {
		String salida;
		try {
			File file = new File(nom);
			Scanner leeArch = new Scanner(file);
      String frase;
			StringBuilder resultado = new StringBuilder();
			int a,b,n;
			n = lee.nextInt();
      for(int i = 0; i < n; i++){
        a = lee.nextInt();
        b = lee.nextInt();
        resultado.append(a+b).append("\n");
			}
				
			}
			
			leeArch.close();
			
		}
		
		catch(Exception e) {
		 salida = "No se puedo abrir.";
		}
		
		return salida.toString();
	}


  public static void main(String[] args) {
      		resultado = leeArchivo(args[0]);
		if (resultado == null) 
			System.out.println("\nError");
		else
			System.out.println("Sumas:\n" + resultado);
	}
    
  }
}

