import java.io.File;
import java.util.Scanner;
public class Tarea01{

  public static String leeArchivo (String nom) {
		String salida;
		try {
			File file = new File(nom);
			Scanner leeArch = new Scanner(file);
      String frase;
			StringBuilder resultado = new StringBuilder();
			int a,b,n;
			n = lee.nextInt();
      for(int i = 0; i < n; i++){
        a = lee.nextInt();
        b = lee.nextInt();
        resultado.append(a+b).append("\n");
			}
				
			}
			
			leeArch.close();
			
		}
		
		catch(Exception e) {
		 salida = "No se puedo abrir.";
		}
		
		return salida.toString();
	}


  public static void main(String[] args) {
    Scanner lee = new Scanner(System.in);
    String nom, resultado;
		System.out.println("Ingresa nombre del archivo"); 
		nom = lee.nextLine();
		resultado = leeArchivo(nom);
		if (resultado == null) 
			System.out.println("\nError");
		else
			System.out.println("Sumas:\n" + resultado);
	}
    
  }
}

